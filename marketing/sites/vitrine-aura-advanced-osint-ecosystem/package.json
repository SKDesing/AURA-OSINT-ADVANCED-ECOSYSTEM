{
  "name": "vitrine-aura-advanced-osint-ecosystem",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/runtime": "^7.28.4",
    "@emotion/styled": "^11.14.1",
    "@react-three/drei": "^9.88.0",
    "@react-three/fiber": "^8.15.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.545.0",
    "nodemailer": "^7.0.9",
    "phosphor-react": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.19",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "concurrently": "^8.2.0",
    "cypress": "^13.6.0",
    "eslint": "^8.57.0",
    "jest": "^30.2.0",
    "prettier": "^3.1.0",
    "react-app-rewired": "^0.1.0",
    "supertest": "^6.3.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:api": "jest __tests__/api.test.js",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "server": "node server.js",
    "dev": "concurrently \"npm start\" \"npm run server\"",
    "deploy": "npm run build && npm run server",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prettier": "prettier --write src/",
    "audit:security": "npm audit",
    "audit:fix": "npm audit fix",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "npm run lint && npm run test:coverage && npm run test:api && npm run audit:security"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "overrides": {
    "nth-check": "^2.0.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^5.2.1"
  }
}
