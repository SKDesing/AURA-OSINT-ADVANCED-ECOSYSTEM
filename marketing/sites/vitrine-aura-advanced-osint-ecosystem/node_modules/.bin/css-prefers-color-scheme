#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/marketing/sites/vitrine-aura-advanced-osint-ecosystem/node_modules/css-prefers-color-scheme/dist/node_modules:/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/marketing/sites/vitrine-aura-advanced-osint-ecosystem/node_modules/css-prefers-color-scheme/node_modules:/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/marketing/sites/vitrine-aura-advanced-osint-ecosystem/node_modules:/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/marketing/sites/node_modules:/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/marketing/node_modules:/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/node_modules:/home/soufiane/node_modules:/home/node_modules:/node_modules:/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/marketing/sites/vitrine-aura-advanced-osint-ecosystem/node_modules/css-prefers-color-scheme/dist/node_modules:/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/marketing/sites/vitrine-aura-advanced-osint-ecosystem/node_modules/css-prefers-color-scheme/node_modules:/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/marketing/sites/vitrine-aura-advanced-osint-ecosystem/node_modules:/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/marketing/sites/node_modules:/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/marketing/node_modules:/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/node_modules:/home/soufiane/node_modules:/home/node_modules:/node_modules:/home/soufiane/AURA-OSINT-ADVANCED-ECOSYSTEM/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../css-prefers-color-scheme/dist/cli.cjs" "$@"
else
  exec node  "$basedir/../css-prefers-color-scheme/dist/cli.cjs" "$@"
fi
