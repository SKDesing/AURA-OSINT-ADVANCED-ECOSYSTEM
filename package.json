{
  "name": "aura-database-maestro",
  "version": "2.0.0",
  "description": "AURA Database Maestro - Moteur d'intelligence forensique cross-plateforme world-class",
  "main": "backend/services/correlation-engine-complete.js",
  "scripts": {
    "start": "node scripts/setup/aura-master-launcher.js",
    "test": "node tests/unit/aura-test-suite.js",
    "benchmark": "node tests/performance/benchmark-suite.js",
    "audit-ports": "node backend/config/port-audit.js",
    "orchestrator": "node scripts/setup/service-orchestrator.js",
    "dev": "nodemon analytics-api.js",
    "build": "echo 'Building AURA Maestro...' && npm test",
    "deploy": "npm run build && echo 'Ready for production deployment'",
    "health": "curl -s http://localhost:4002/api/analytics/dashboard | jq .",
    "install-deps": "npm install && echo 'Dependencies installed for world-class performance'",
    "migrate-chromium": "node scripts/setup/migrate-to-chromium.js",
    "test-migration": "node tests/unit/test-chromium-migration.js",
    "install-chromium": "./scripts/setup/install-chromium.sh",
    "validate-setup": "npm run test-migration && npm run benchmark",
    "full-setup": "npm run migrate-chromium && npm run install-chromium && npm run validate-setup",
    "quick-start": "node scripts/setup/quick-start.js",
    "gui": "node scripts/setup/startup-orchestrator.js",
    "scan-browser-calls": "node scripts/setup/chromium-only-enforcer.js scan",
    "fix-browser-calls": "node scripts/setup/chromium-only-enforcer.js fix",
    "enforce-chromium": "node scripts/setup/chromium-only-enforcer.js scan && echo 'Scan terminé - Voir résultats ci-dessus'",
    "prepare": "husky install",
    "compliance-check": "node scripts/setup/chromium-only-enforcer.js scan",
    "security-setup": "./security-setup.sh",
    "security-status": "node backend/services/security-manager.js status",
    "security-report": "node backend/services/security-manager.js report",
    "ui-sync": "node scripts/ui-sync.js sync",
    "ui-check": "node scripts/ui-sync.js check",
    "ui-report": "node scripts/ui-sync.js report",
    "ui-cleanup": "node scripts/ui-sync.js cleanup",
    "prod:build": "docker-compose -f docker-compose.prod.yml build",
    "prod:up": "docker-compose -f docker-compose.prod.yml up -d",
    "prod:down": "docker-compose -f docker-compose.prod.yml down",
    "prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "prod:deploy": "npm run prod:build && npm run prod:up"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "fast-levenshtein": "^3.0.0",
    "helmet": "^7.1.0",
    "natural": "^6.12.0",
    "pg": "^8.11.3",
    "puppeteer": "^21.5.0",
    "rate-limiter-flexible": "^2.4.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/node": "^20.8.0",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.1",
    "playwright": "^1.56.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "optionalDependencies": {
    "neo4j-driver": "^5.13.0",
    "sentence-transformers": "^1.0.0",
    "tensorflow": "^4.10.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "forensic",
    "osint",
    "correlation",
    "cross-platform",
    "identity-matching",
    "social-networks",
    "machine-learning",
    "nlp",
    "graph-analysis",
    "cybersecurity"
  ],
  "author": "Kaabache Soufiane <contact@tiktokliveanalyser.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SKDesing/TikTok-Live-Analyser.git"
  },
  "bugs": {
    "url": "https://github.com/SKDesing/TikTok-Live-Analyser/issues"
  },
  "homepage": "https://github.com/SKDesing/TikTok-Live-Analyser#readme"
}
