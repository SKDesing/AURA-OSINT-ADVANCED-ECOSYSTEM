{
  "name": "tiktok-live-analyser",
  "version": "2.0.0",
  "description": "TikTok Live Analyser - Solution Forensique Professionnelle",
  "main": "app-launcher.js",
  "scripts": {
    "start": "node app-launcher.js",
    "dev": "node app-launcher.js",
    "install-all": "npm install && cd live-tracker && npm install && cd ../frontend-react && npm install",
    "backend": "cd live-tracker && node enhanced-server.js",
    "frontend": "cd frontend-react && npm start",
    "scraper": "cd live-tracker && node tiktok-scraper-advanced.js",
    "docker:build": "./docker-scripts/build.sh",
    "docker:start": "./docker-scripts/start.sh",
    "docker:stop": "./docker-scripts/stop.sh",
    "docker:logs": "docker-compose -f docker-compose.scis.yml logs -f",
    "docker:restart": "docker-compose -f docker-compose.scis.yml restart",
    "microservices:build": "./docker-scripts/build-microservices.sh",
    "microservices:start": "docker-compose -f docker-compose.microservices.yml up -d",
    "microservices:stop": "docker-compose -f docker-compose.microservices.yml down",
    "microservices:logs": "docker-compose -f docker-compose.microservices.yml logs -f",
    "microservices:restart": "docker-compose -f docker-compose.microservices.yml restart"
  },
  "dependencies": {
    "open": "^8.4.0"
  },
  "devDependencies": {},
  "keywords": ["tiktok", "forensic", "scraper", "live", "analysis"],
  "author": "AURA Forensic Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/tiktok-live-analyser"
  }
}