{
  "name": "@aura/osint-advanced-ecosystem",
  "version": "2.0.0",
  "description": "AURA TikTok Intelligence Engine - Professional OSINT Platform for TikTok Live Analysis",
  "main": "backend/api/analytics-api.js",
  "scripts": {
    "start": "node scripts/setup/startup-orchestrator.js",
    "test": "vitest run --reporter=junit --coverage",
    "benchmark": "node tests/performance/benchmark-suite.js",
    "audit-ports": "node backend/config/port-audit.js",
    "analytics": "node backend/api/analytics-api.js",
    "dev": "pnpm -C apps/aura-browser dev",
    "dev:browser": "pnpm -C apps/aura-browser dev",
    "build:ui": "pnpm --filter ./clients/web-react build",
    "build:browser": "pnpm -C apps/aura-browser build",
    "start:chromium-ui": "echo 'Deprecated: use pnpm run dev (Electron)'; exit 1",
    "build": "echo 'Building AURA Maestro...' && npm test",
    "deploy": "npm run build && echo 'Ready for production deployment'",
    "health": "node monitoring/health-checks.js",
    "install-deps": "npm install && echo 'Dependencies installed for world-class performance'",
    "migrate-chromium": "node scripts/setup/migrate-to-chromium.js",
    "test-migration": "node tests/unit/test-chromium-migration.js",
    "install-chromium": "./scripts/setup/install-chromium.sh",
    "validate-setup": "npm run test-migration && npm run benchmark",
    "full-setup": "npm run migrate-chromium && npm run install-chromium && npm run validate-setup",
    "gui": "node scripts/setup/startup-orchestrator.js",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts,.tsx,.js --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "typecheck": "tsc --noEmit",
    "compliance-check": "node scripts/setup/chromium-only-enforcer.js scan",
    "backup": "node backups/backup-scheduler.js",
    "monitor": "node monitoring/system-monitor.js",
    "test:monitor": "node tests/unit/test-system-monitor.js",
    "test:engine-base": "node tests/unit/test-engine-base.js",
    "security-setup": "./security-setup.sh",
    "security-status": "node backend/services/security-manager.js status",
    "security-report": "node backend/services/security-manager.js report",
    "ui-sync": "node scripts/ui-sync.js sync",
    "ui-check": "node scripts/ui-sync.js check",
    "ui-report": "node scripts/ui-sync.js report",
    "ui-cleanup": "node scripts/ui-sync.js cleanup",
    "prod:build": "docker-compose -f docker-compose.prod.yml build",
    "prod:up": "docker-compose -f docker-compose.prod.yml up -d",
    "prod:down": "docker-compose -f docker-compose.prod.yml down",
    "prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "prod:deploy": "npm run prod:build && npm run prod:up",
    "monitoring:up": "docker-compose -f monitoring/docker-compose.monitoring.yml up -d",
    "monitoring:down": "docker-compose -f monitoring/docker-compose.monitoring.yml down",
    "architecture:validate": "node scripts/architecture-validator.js",
    "tiktok:start": "node engines/tiktok/TikTokEngine.js",
    "platform:status": "node -e \"console.log(require('./EXPANSION-ROADMAP.md'))\"",
    "roadmap": "cat EXPANSION-ROADMAP.md",
    "roadmap:implement": "node scripts/roadmap-implementation.js",
    "audit:codebase": "./scripts/audit-codebase.sh",
    "audit:merge": "node scripts/analyze-merge-candidates.js",
    "harassment:test": "node -e \"const detector = require('./ai/models/harassment-detector'); detector.analyze('test message').then(console.log)\"",
    "forensic:test": "node -e \"const analyzer = require('./algorithms/forensic-timeline-analyzer'); console.log('Forensic analyzer loaded')\"",
    "anti-harassment:start": "node -e \"const engine = require('./engines/anti-harassment/AntiHarassmentEngine'); new engine().initialize().then(() => console.log('Anti-harassment engine ready'))\"",
    "scan-browser-calls": "node scripts/ci/scan-browser-violations.js",
    "fix-browser-calls": "node scripts/ci/fix-browser-violations.js",
    "test:chromium": "node tests/unit/test-chromium-migration.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix --force",
    "dev-safe": "node --max-old-space-size=8192 server.js",
    "debug-safe": "node --inspect --max-old-space-size=8192 server.js",
    "prod": "pm2 start server.js --name aura-osint --max-memory-restart 1500M",
    "llm:install": "bash scripts/ai/install-llama-cpp.sh",
    "llm:download:qwen": "bash ai/local-llm/scripts/download-qwen2-1_5b.sh",
    "llm:run:qwen": "bash ai/local-llm/scripts/run-llm-qwen.sh",
    "llm:bench:qwen": "bash scripts/ai/bench-qwen.sh",
    "ai:gateway": "ts-node ai/gateway/src/main.ts",
    "ai:harassment:test": "node ai/engines/harassment/tests/engine.test.js",
    "ai:harassment:test:enhanced": "node ai/engines/harassment/tests/engine-enhanced.test.js",
    "inventory": "node scripts/analysis/front-inventory.js --markdown --json",
    "inventory:check": "node scripts/analysis/check-thresholds.js",
    "inventory:full": "npm run inventory && npm run inventory:check",
    "ai:registry:diff": "node scripts/ai/registry-diff.js",
    "ai:metrics": "node scripts/run/quick-metrics.js --json",
    "ai:health": "curl -s http://localhost:3001/ai/health | jq",
    "ai:quality:gate": "node scripts/ai/quality-gate.js",
    "ai:preintel:validate": "node scripts/ai/validate-preintel.js",
    "ai:preintel:test": "jest ai/gateway/src/preintel/preintel.test.ts",
    "ai:router:validate": "node scripts/ai/router/validate-router.js",
    "ai:router:bench": "node scripts/audit/ai/router-bench.js",
    "ai:router:test": "jest ai/gateway/src/router/decision-engine.test.ts",
    "ai:embeddings:health": "node scripts/audit/ai/embeddings-health.js",
    "ai:embeddings:build-index": "node scripts/ai/router/build-embeddings.js",
    "docs:check-links": "node scripts/audit/check-links.js",
    "audit:create-issues": "node scripts/audit/create-audit-issues.js",
    "ports:inventory": "node scripts/dev/port-inventory.js",
    "ports:fix": "node scripts/dev/port-fix.js --apply",
    "ports:kill": "bash scripts/dev/kill-port.sh",
    "mvp:backend": "node backend/mvp-server-fixed.js",
    "mvp:frontend": "cd clients/web-react && pnpm start",
    "mvp:start": "npm run ports:fix && concurrently \"npm run mvp:backend\" \"npm run mvp:frontend\"",
    "mvp:dev": "npm run ports:inventory && npm run mvp:start",
    "audit:full": "node scripts/audit/ai/full-audit.js",
    "ai:validate:manifest": "node scripts/audit/ai/validate-manifest.js",
    "audit:ports": "npm run ports:inventory",
    "audit:security": "npm audit --audit-level=moderate && npm run security:audit",
    "audit:report": "npm run audit:full && echo 'Reports in reports/audit/'",
    "test:unit": "vitest run --coverage",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:a11y": "lighthouse http://localhost:3000 --quiet --only-categories=accessibility",
    "test:all": "pnpm run test:unit && pnpm run test:e2e"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cosine-similarity": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "fast-levenshtein": "^3.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "mysql2": "^3.15.1",
    "natural": "^6.12.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "puppeteer": "^24.23.0",
    "rate-limiter-flexible": "^2.4.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/node": "^20.8.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "cypress": "^15.4.0",
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3",
    "electron-is-dev": "^3.0.1",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "jest": "^30.2.0",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.1",
    "playwright": "^1.56.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.1"
  },
  "optionalDependencies": {
    "neo4j-driver": "^5.13.0",
    "sentence-transformers": "^1.0.0",
    "tensorflow": "^4.10.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "forensic",
    "osint",
    "correlation",
    "cross-platform",
    "identity-matching",
    "social-networks",
    "machine-learning",
    "nlp",
    "graph-analysis",
    "cybersecurity"
  ],
  "author": "Kaabache Soufiane <contact@aura-osint.com>",
