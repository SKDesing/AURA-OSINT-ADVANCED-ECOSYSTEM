{
  "name": "@aura/osint-advanced-ecosystem",
  "version": "2.0.0",
  "description": "AURA TikTok Intelligence Engine - Professional OSINT Platform for TikTok Live Analysis",
  "main": "backend/api/analytics-api.js",
  "scripts": {
    "start": "node scripts/setup/startup-orchestrator.js",
    "test": "node tests/unit/aura-test-suite.js",
    "benchmark": "node tests/performance/benchmark-suite.js",
    "audit-ports": "node backend/config/port-audit.js",
    "analytics": "node backend/api/analytics-api.js",
    "dev": "nodemon backend/api/analytics-api.js",
    "build": "echo 'Building AURA Maestro...' && npm test",
    "deploy": "npm run build && echo 'Ready for production deployment'",
    "health": "node monitoring/health-checks.js",
    "install-deps": "npm install && echo 'Dependencies installed for world-class performance'",
    "migrate-chromium": "node scripts/setup/migrate-to-chromium.js",
    "test-migration": "node tests/unit/test-chromium-migration.js",
    "install-chromium": "./scripts/setup/install-chromium.sh",
    "validate-setup": "npm run test-migration && npm run benchmark",
    "full-setup": "npm run migrate-chromium && npm run install-chromium && npm run validate-setup",
    "gui": "node scripts/setup/startup-orchestrator.js",
    "prepare": "husky install",
    "compliance-check": "node scripts/setup/chromium-only-enforcer.js scan",
    "backup": "node backups/backup-scheduler.js",
    "monitor": "node monitoring/system-monitor.js",
    "test:monitor": "node tests/unit/test-system-monitor.js",
    "test:engine-base": "node tests/unit/test-engine-base.js",
    "security-setup": "./security-setup.sh",
    "security-status": "node backend/services/security-manager.js status",
    "security-report": "node backend/services/security-manager.js report",
    "ui-sync": "node scripts/ui-sync.js sync",
    "ui-check": "node scripts/ui-sync.js check",
    "ui-report": "node scripts/ui-sync.js report",
    "ui-cleanup": "node scripts/ui-sync.js cleanup",
    "prod:build": "docker-compose -f docker-compose.prod.yml build",
    "prod:up": "docker-compose -f docker-compose.prod.yml up -d",
    "prod:down": "docker-compose -f docker-compose.prod.yml down",
    "prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "prod:deploy": "npm run prod:build && npm run prod:up",
    "monitoring:up": "docker-compose -f monitoring/docker-compose.monitoring.yml up -d",
    "monitoring:down": "docker-compose -f monitoring/docker-compose.monitoring.yml down",
    "architecture:validate": "node scripts/architecture-validator.js",
    "tiktok:start": "node engines/tiktok/TikTokEngine.js",
    "platform:status": "node -e \"console.log(require('./EXPANSION-ROADMAP.md'))\"",
    "roadmap": "cat EXPANSION-ROADMAP.md",
    "roadmap:implement": "node scripts/roadmap-implementation.js",
    "audit:codebase": "./scripts/audit-codebase.sh",
    "audit:merge": "node scripts/analyze-merge-candidates.js",
    "harassment:test": "node -e \"const detector = require('./ai/models/harassment-detector'); detector.analyze('test message').then(console.log)\"",
    "forensic:test": "node -e \"const analyzer = require('./algorithms/forensic-timeline-analyzer'); console.log('Forensic analyzer loaded')\"",
    "anti-harassment:start": "node -e \"const engine = require('./engines/anti-harassment/AntiHarassmentEngine'); new engine().initialize().then(() => console.log('Anti-harassment engine ready'))\"",
    "scan-browser-calls": "node scripts/ci/scan-browser-violations.js",
    "fix-browser-calls": "node scripts/ci/fix-browser-violations.js",
    "test:chromium": "node tests/unit/test-chromium-migration.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix --force",
    "dev-safe": "node --max-old-space-size=8192 server.js",
    "debug-safe": "node --inspect --max-old-space-size=8192 server.js",
    "prod": "pm2 start server.js --name aura-osint --max-memory-restart 1500M",
    "llm:download:qwen": "bash ai/local-llm/scripts/download-qwen2-1_5b.sh",
    "llm:run:qwen": "bash ai/local-llm/scripts/run-llm-qwen.sh",
    "llm:test:qwen": "bash ai/local-llm/scripts/test-qwen.sh",
    "ai:gateway": "ts-node ai/gateway/src/main.ts",
    "ai:harassment:test": "node ai/engines/harassment/tests/engine.test.js"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "fast-levenshtein": "^3.0.0",
    "helmet": "^7.1.0",
    "mysql2": "^3.15.1",
    "natural": "^6.12.0",
    "pg": "^8.11.3",
    "puppeteer": "^24.23.0",
    "rate-limiter-flexible": "^2.4.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/node": "^20.8.0",
    "cypress": "^15.4.0",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.1",
    "playwright": "^1.56.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "optionalDependencies": {
    "neo4j-driver": "^5.13.0",
    "sentence-transformers": "^1.0.0",
    "tensorflow": "^4.10.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "forensic",
    "osint",
    "correlation",
    "cross-platform",
    "identity-matching",
    "social-networks",
    "machine-learning",
    "nlp",
    "graph-analysis",
    "cybersecurity"
  ],
  "author": "Kaabache Soufiane <contact@aura-osint.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SKDesing/AURA-OSINT-ADVANCED-ECOSYSTEM.git"
  },
  "bugs": {
    "url": "https://github.com/SKDesing/AURA-OSINT-ADVANCED-ECOSYSTEM/issues"
  },
  "homepage": "https://github.com/SKDesing/AURA-OSINT-ADVANCED-ECOSYSTEM#readme"
}
