  "main": "app-launcher.js",
  "scripts": {
    "start": "node app-launcher.js",
    "dev": "npm run backend & npm run frontend",
    "backend": "cd live-tracker && node enhanced-server.js",
    "frontend": "cd frontend-react && npm start",
    "build": "./scripts/build/build.sh",
    "deploy": "./scripts/deployment/LANCER-APPLICATION.sh",
    "docker:up": "docker-compose -f config/docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f config/docker/docker-compose.yml down",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd live-tracker && npm test",
    "test:frontend": "cd frontend-react && npm test",
