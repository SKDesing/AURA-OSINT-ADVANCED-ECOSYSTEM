# AURA OSINT - Configuration Sécurisée
# Copier vers .env et remplir avec les vraies valeurs

# Base de données
DB_HOST=localhost
DB_PORT=5432
DB_NAME=aura_forensic
DB_USER=postgres
DB_PASSWORD=your_secure_password_here
DB_CONNECTION=postgresql://user:pass@localhost:5432/aura_forensic

# API & Sécurité
API_KEY=your_api_key_here
SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here
API_TOKEN=your_api_token_here

# Services externes
TIKTOK_API_KEY=your_tiktok_api_key
INSTAGRAM_TOKEN=your_instagram_token
TWITTER_BEARER_TOKEN=your_twitter_token

# Monitoring
ALERT_WEBHOOK=your_webhook_url
SLACK_TOKEN=your_slack_token
